<Application x:Class="UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:UI">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/ToolBarButtons.xaml"/>
                <ResourceDictionary Source="Themes/TextboxTheme.xaml"/>
                <ResourceDictionary Source="Themes/RichtextboxTheme.xaml"/>
                <ResourceDictionary Source="Themes/HomePageButtons.xaml"/>
                <ResourceDictionary Source="Themes/HomePageButtons_Grey.xaml"/>
                <ResourceDictionary Source="Themes/HomePageButtons_Blue.xaml"/>
                <ResourceDictionary Source="Themes/MainRichtextboxTheme.xaml"/>
                <ResourceDictionary Source="Themes/TextButton.xaml"/>
                <ResourceDictionary Source="Themes/CloseButtonTheme.xaml"/>
                <ResourceDictionary Source="Themes/Defaults.xaml"/>
                <ResourceDictionary>
                    <local:Bootstrapper x:Key="Bootstrapper"/>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
